{
  "name": "scrypted-nvr-backup",
  "version": "1.0.0",
  "description": "Automated backup script for Scrypted NVR to Google Cloud Storage",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/backup.js",
    "dev": "ts-node src/backup.ts",
    "test": "npm run test:unit",
    "test:unit": "ts-node tests/backup.test.ts",
    "test:smoke": "ts-node src/test-scrypted.ts",
    "verify": "ts-node src/verify.ts",
    "lint": "eslint --config .eslintrc.json . --ext .ts",
    "lint:fix": "eslint --config .eslintrc.json . --ext .ts --fix"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.12.1",
    "@scrypted/sdk": "^0.0.108",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Kilo Code",
  "license": "MIT"
}
